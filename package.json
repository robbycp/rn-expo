{
  "name": "rn-expo-template",
  "version": "0.1.0",
  "scripts": {
    "lint": "eslint --fix --cache \"src/**/*.{js,jsx,ts,tsx}\"",
    "start": "expo start --dev-client",
    "eas-build-pre-install": "sh ./scripts/eas-build-pre-install.sh",
    "build:android:dev": "eas build --profile dev --platform android",
    "build:android:staging": "eas build --profile staging --platform android",
    "build:android:pre-production": "eas build --profile pre-production --platform android",
    "build:android:production": "eas build --profile production --platform android",
    "update:staging": "eas update --branch staging --message $1",
    "update:production": "eas update --branch production --message $1",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo"
  },
